        $('[data-toggle="popover"]').popover('hide');
      }
      $("video").bind('timeupdate', pausingFunction);
      function pausingFunction(){
        if(this.currentTime>10.5){
          this.pause();
          $("#timeShow").fadeIn('slow');
        }
      }
      $(".btn-warning").hover(function(){
        $(this).css('background-color', 'white');
      });
      $(".btn-warning").mouseout(function(){
        $(this).css('background-color', '');
      });
      function defineloc(){
        if(window.location.hash === "#events"){
          prePageOpen();
          eventsOpen();
        }
        else if(window.location.hash === "#about-us"){
          prePageOpen();
          aboutOpen();
        }
        else if(window.location.hash === "#gallery"){
          prePageOpen();
          galleryOpen();
        }
        else if(window.location.hash === "#team"){
          prePageOpen();
          teamOpen();
        }
        else if(window.location.hash === "#accomodation"){
          prePageOpen();
          accomodationOpen();
        }
      }
      var charms = $("#el").data('charms');
      $("#galleryThumbnails>div>div>img").click(function(){
        var x = $(this).attr('data-value');
        if(x > $(".carousel-inner>.carousel-item").length){
          $(".carousel-inner>.carousel-item:eq(1)").addClass('active');
        }
        else{
          $(".carousel-inner>.carousel-item:eq("+(x-1)+")").addClass('active');
        }
        $("#galleryThumbnails").fadeOut('fast');
        $("#galleryCarousel").fadeIn('fast');
        charms.close();
      });
      $(".fa-times").click(galleryFunction);
      function galleryFunction(){
        $(".carousel-inner>.active").removeClass('active');
        $("#galleryCarousel").fadeOut('fast', function(){
          $("#galleryThumbnails").fadeIn('fast');
        });
        charms.open();
      }
      function prePageOpen(){
        $("video").hide();
        galleryFunction();
        $("body").css('height', 'auto');
        $("html").css('height', 'auto');
        $('#contact').modal('hide');
        $('#sponsors').modal('hide');
        $("#overlay").hide();
        charms.close();
        $("body").css('overflow-y', 'auto');
        $("#events").css('display', 'none');
        $("#gallery").css('display', 'none');
        $("#team").css('display', 'none');
        $("#accomodation").css('display', 'none');
        $("#about-us").css('display', 'none');
      }
      function aboutOpen(){
        window.location = "#about-us";
        $("body").css('overflow-y', 'scroll');
        $("body").css('height', '100%');
        $("html").css('height', '100%');
        $(window).scrollLock = true;
        $("#about-us").css('display', 'block');
        $("#about-us").addClass('open-page');
        $(".card").addClass('opened');
      }
      function eventsOpen(){
        window.location = "#events";
        $("#events").css('display', 'block');
        $("#events").addClass('open-page');
      }
      function galleryOpen(){
        window.location = "#gallery";
        $("#gallery").addClass('open-page');
        $("#gallery").css('display', 'block');
      }
      function teamOpen(){
        window.location = "#team";
        $("#team").addClass('open-page');
        $("#team").css('display', 'block');
        $(window).scrollLock = true;
      }
      function accomodationOpen(){
        window.location = "#accomodation";
        $("#accomodation").addClass('open-page');
        $("#accomodation").css('display', 'block');
      }
      $("#desktop").click(function(){
        prePageOpen();
        window.location.hash="";
        $("video").show();
        $("video").currentTime = 0;
        $("video")[0].play();
        $("#overlay").show();
      });
      $(".large>div").bind('click', function(){
        $(".large>div").css('border', '0px');
        $(this).addClass('jumpwin8');
        var element = $(this);
        $(this).css('border', '5px solid #1A5F6A');
        if(element.attr('id') === 'about-tile'){
          window.location = "#about-us";
        }
        else if(element.attr('id') === 'event-tile'){
          window.location = "#events";
        }
        else if(element.attr('id') === 'team-tile'){
          window.location = "#team";
        }
        else if(element.attr('id') === 'hospitality-tile'){
          window.location = "#accomodation";
        }
        else if(element.attr('id') === 'gallery-tile'){
          window.location = "#gallery";
        }
        setTimeout(function(){$(".large>div").removeClass('jumpwin8');}, 2200);
      });
      $(document).click(function(e){
        if(e.which !== undefined){
        $('[data-toggle="popover"]').popover('hide');
        $("body").css('overflow-y', 'auto');
        if($("#el")[0] != e.target && $("#startmenu")[0] != e.target){
          var charms = $("#el").data('charms');
          charms.close();
        }
      }
      });
      var i = 0;
      var scrolledTop = false;
      $(document).scroll(function(e){
        e.preventDefault();
        e.stopPropagation();
        var lastPosition = $(this).scrollTop();
        if(lastPosition>i){
          if(!scrolledTop){
            if($("#about-us").css('display') === 'block'){
              $("#showabout").fadeOut('slow', function(){
                $("#showmap").fadeIn('slow');
              });
            }
            scrolledTop = true;
          }
        }
        else{
          if(scrolledTop){
            if($("#about-us").css('display') === 'block'){
              $("#showmap").fadeOut('slow', function(){
                $("#showabout").fadeIn('slow');
              });
            }
            scrolledTop = false;
          }
        }
        i = lastPosition;
      });
      $("#startmenu").click(function(){
        var charms = $("#el").data('charms');
        charms.toggle();
      });
      setTimeout(function(){
        $(".preloader").css({
          transform: 'rotateY(90deg)',
          opacity : 0,
          transition : 'transform 1s ease-in-out, opacity 1s'
        });
        $("#main").css('opacity', '0');
      }, 6000);
      setTimeout(function(){
        $(".preloader").css('display', 'none');
        $(".container-fluid").addClass('h-100');
        $("html,body").css('height', 'auto');
        $("body").css('overflow-y', 'hidden');
        $("#overlay").show();
        $("video").show();
        if($(window).width()>=500){
        }
        $("#main-screen").css({
          opacity: 1,
          transition : 'opacity 1s'
        });
        $("#overlay").css('display', 'block');
        var charms = $("#start").data('charms');
        charms.open();
        setTimeout(function(){$('[data-toggle="popover"]').popover();
        $('[data-toggle="popover"]').popover('show');/*$("video")[0].play();*/}, 500);
      }, 7500);
