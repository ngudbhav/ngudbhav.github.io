    <script type="text/javascript">
      var noOfParticipants = 0;
      var isJIIT = 0;
      var yearv = 0;
      function participants(value){
        noOfParticipants = value
          if(value == 1){
            $('#participant1').show();$('#participant2').hide();$('#participant3').hide();$('#participant4').hide();
            $('#participant2>.form-group>input').val('');$('#participant3>.form-group>input').val('');$('#participant4>.form-group>input').val('');
          }
          else if(value == 2){
            $('#participant2').show();$('#participant3').hide();$('#participant4').hide();
            $('#participant3>.form-group>input').val('');$('#participant4.form-group>input').val('');

          }
          else if(value == 3){
            $('#participant2').show();$('#participant3').show();$('#participant4').hide();
            $('#participant4>.form-group>input').val('');
          }
          else if(value == 4){
            $('#participant2').show();$('#participant3').show();$('#participant4').show();
          }
        }
        function college(value){
          isJIIT = value;
          if(value == 3){
            $("#onenrollc").hide();
            $("#collegeField").fadeIn('fast');
          }
          else{
            $("#onenrollc").show();
            $("#collegename").val('');
            $("#collegeField").fadeOut('fast');
          }
        }
        function year(value){
          yearv = value;
        }
    </script>
